
<table border="1">

    <thead>
        <tr>
            <th>Nombres y apellidos</th>
            <th>Tipo</th>
            <th>Email</th>
            <th>Telefono</th>
            <th>Ciudad</th>
            <th>Voucher</th>
{{!--             <th> Eliminar</th> --}}
        </tr>
    </thead>
    <tbody>
        {{#each users}}
            <tr>
                <td> {{this.first_name}} {{this.last_name}} </td>
                <td> {{this.type}} </td>
                <td> {{this.email}} </td>
                <td> {{this.phone}} </td>
                <td> {{this.city}} </td>
                <td>
                    <a href="{{this.voucher}}">
                        <img src="{{this.voucherThumb}}">
                    </a>
                </td>
             {{!--    <td>
                    <a href="/eliminameuno/{{this.id}}">Eliminar</a>
                </td> --}}
            </tr>
        {{/each}}
    </tbody>
</table>